HISTORY
{
	ID (PRIMARY KYE)
	URL
	LOCAL_PATH
	STATUS
	{
		ACCEPTED (A)
		PROCESSING (I)
		PROCESSED (D)
	}
	CATEGORY
	{
		PAGE (T)
		ATTACHEMENT (B)
	}	
	LAST_MODIFIED_TIME
	MD5	
}

POST
{
	POST_ID (PRIMARY KEY)
	URL
	MAIN_MESSAGE_ID
	TYPE
	{
		KB
		EXAMPLE
		DISCUSSION
	}
	TITLE
}

MESSAGE
{
	MSG_ID (PRIMAMRY KEY)
	POST_ID (FORIEN KEY)
	AUTH_ID (FORIEN KEY)
	URL
	LAST_MODIFIED_TIME
	KUDOS
	CATEGORY
	{
		QUESTION
		REPLAY
		SOLUTION
	}
	CONTENT
	IS_SOLUTION
	EST_SULUTION
}

ATTACHMENT
{
	ATT_ID (PRIMARY KEY)
	MSG_ID (FORIEN KEY)	
	URL
	LOCAL_PATH
	CATEGORY
	{
		VI
		PDF
		IMAGE
		ZIP
		CODE
		OTHER
	}
}

AUTH
{
	AUTH_ID (PRIMORY KEY)
	AUTH_NAME
}



